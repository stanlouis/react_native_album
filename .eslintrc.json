{
  "extends": "airbnb",
  "plugins": ["react", "react-native"],
  "rules": {
    "no-use-before-define": "off",
    "react/jsx-filename-extension": "off",
    "react/prop-types": "off",
    "comma-dangle": "off",
    "quotes": ["error", "double"],
    "import/no-named-as-default": 0
  },
  "globals": {
    "fetch": false
  }
}
